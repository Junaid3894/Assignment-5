/*Write a program to print squares of the first N natural numbers*/
#include<stdio.h>
int main()
{
int n;
int i;
int square;
// Input how many square you want to see
printf("Enter the n :");
scanf("%d",&n);
// start loop from  1 and increment it by 1 in each iteration till n
for(i=1;i<=n;i++){
    square=i*i;
    printf("%d\n",square);

}
return 0;
}
