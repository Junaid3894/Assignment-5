/*Write a program to print the first N natural numbers in reverse order*/
#include<stdio.h>
int main()
{
int n;
int i;
printf("Enter the nth term which you want to see :");
scanf("%d",&n);
// start loop counter from 10,decrement by 1 in each iteration unitl condition is True
for(i=n;i>=1;i--){
    printf("%d\n",i);
}
return 0;
}
